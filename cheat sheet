

#create azure auth:  (required for automatic deployment of resources)
az ad sp create-for-rbac --sdk-auth > azure.auth

#Compile project:
mvn compile assembly:single

#Deploy project
mvn compile package azure-webapp:deploy



#Create resources:
java -cp target/scc2122-mgt-1.0-jar-with-dependencies.jar scc.mgt.AzureManagement

#Delete resources:
java -cp target/scc2122-mgt-1.0-jar-with-dependencies.jar scc.mgt.AzureManagement -delete




#Command to set app name:
az functionapp config appsettings set --name scc2122app-westeurope-n49067n52405n45081 --resource-group westeurope